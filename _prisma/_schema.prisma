generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

model post {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  slug  String
  title String
}

model user {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  name  String
}
